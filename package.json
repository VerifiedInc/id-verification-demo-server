{
  "name": "feathers-mikro-orm-starter",
  "description": "A starter for web apps using Feathers and MikroORM",
  "version": "0.0.1",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Unum ID",
    "email": "devops@unumid.co"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^14.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "npm run lint && npm run compile && NODE_ENV=test npm run migrate && NODE_ENV=test npm run jest",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev --files --no-notify src/",
    "start": "npm run migrate && node lib/",
    "jest": "jest  --forceExit --max-workers=1",
    "compile": "shx rm -rf lib/ && tsc",
    "migrate": "npm run migration:up",
    "migration:create": "npx mikro-orm migration:create",
    "migration:up": "npx mikro-orm migration:up",
    "migration:down": "npx mikro-orm migration:down"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.11",
    "@feathersjs/authentication-local": "^4.5.11",
    "@feathersjs/authentication-oauth": "^4.5.11",
    "@feathersjs/configuration": "^4.5.11",
    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/rest-client": "^4.5.12",
    "@feathersjs/socketio": "^4.5.11",
    "@feathersjs/transport-commons": "^4.5.11",
    "@mikro-orm/cli": "^4.5.9",
    "@mikro-orm/core": "^4.5.9",
    "@mikro-orm/postgresql": "^4.5.9",
    "@mikro-orm/reflection": "^4.5.9",
    "@unumid/server-sdk": "git+ssh://git@github.com/UnumID/server-sdk-typescript.git#rec",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "feathers-alive-ready": "^1.2.2",
    "feathers-hooks-common": "^5.0.6",
    "feathers-mikro-orm": "git+ssh://git@github.com/UnumID/feathers-mikro-orm.git#v0.5.3",
    "helmet": "^4.2.0",
    "newrelic": "^8.9.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "serve-favicon": "^2.5.0",
    "twilio": "^3.77.2",
    "uuid": "^8.3.2",
    "winston": "^3.0.0",
    "winston-syslog": "^2.4.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.9",
    "@types/feathersjs__rest-client": "^1.3.4",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/newrelic": "^7.0.3",
    "@types/node-fetch": "^2.5.7",
    "@types/semver": "^7.3.9",
    "@types/serve-favicon": "^2.5.2",
    "@types/uuid": "^8.3.0",
    "@types/winston-syslog": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@unumid/id-verification-types": "git+ssh://git@github.com:UnumID/id-verification-types.git#v0.1.0",
    "@unumid/types": "git+ssh://git@github.com/UnumID/types#3.50.0",
    "axios": "^0.21.1",
    "eslint": "^7.13.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.0.5"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./build/mikro-orm.config.js"
    ]
  }
}
